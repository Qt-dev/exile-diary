{
  "name": "exile-diary",
  "version": "0.3.10",
  "main": "main.js",
  "build": {
    "appId": "com.pathofexile.exilediary",
    "productName": "Exile Diary",
    "win": {
      "target": "nsis",
      "artifactName": "exile-diary-setup-${version}.${ext}",
      "icon": "./res/img/icons/win/ExileDiary.ico",
      "extraResources": [
        "eng.traineddata"
      ]
    },
    "nsis": {
      "oneClick": "false",
      "allowElevation": "false",
      "allowToChangeInstallationDirectory": "true",
      "installerIcon": "./res/img/icons/win/ExileDiary.ico",
      "uninstallerIcon": "./res/img/icons/win/ExileDiary.ico",
      "installerHeaderIcon": "./res/img/icons/win/ExileDiary.ico"
    },
    "linux": {
      "target": "deb",
      "executableName": "Exile-Diary",
      "icon": "./res/img/icons/png",
      "category": "Utility",
      "maintainer": "briansd9"
    }
  },
  "scripts": {
    "start": "electron .",
    "postinstall": "electron-builder install-app-deps",
    "pack": "electron-builder --dir",
    "dist": "electron-builder"
  },
  "keywords": [
    "util",
    "functional",
    "server",
    "client",
    "browser"
  ],
  "author": "joshi",
  "repository": {
    "type": "git",
    "url": "https://github.com/briansd9/exile-diary.git"
  },
  "contributors": [],
  "dependencies": {
    "@electron/remote": "^2.0.8",
    "chokidar": "^3.5.3",
    "color-convert": "^2.0.1",
    "electron-is-dev": "^2.0.0",
    "electron-overlay-window": "^2.0.1",
    "electron-updater": "^5.2.1",
    "fast-equals": "^4.0.1",
    "imgur": "^2.2.0",
    "jimp": "^0.16.1",
    "js-levenshtein": "^1.1.6",
    "merge-img": "^2.1.3",
    "moment": "^2.29.4",
    "moment-duration-format": "^2.3.2",
    "nodejs-tail": "^1.1.1",
    "opn": "^6.0.0",
    "pastebin-js": "^1.0.6",
    "ps-list": "^7.2.0",
    "request": "^2.88.2",
    "sqlite3": "^5.0.11",
    "tesseract.js": "^3.0.2",
    "winston": "^3.8.1",
    "xlsx": "^0.18.5"
  },
  "devDependencies": {
    "electron": "^20.0.3",
    "electron-builder": "^23.3.3"
  }
}
